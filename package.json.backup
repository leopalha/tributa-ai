  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:alt": "next dev -p 3001",
    "clean-dev": "npx kill-port 3000 && next dev",
    "clean-dev:alt": "npx kill-port 3001 && next dev -p 3001",
    "clean-start": "node clean-and-start.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "seed": "ts-node -r dotenv/config -r tsconfig-paths/register --project tsconfig.scripts.json src/scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:setup": "npx prisma generate && npx prisma db push",
    "db:reset": "npx prisma db push --force-reset && npx prisma db seed",
    "db:backfill-embeddings": "tsx scripts/backfill-embeddings.ts",
    "check-obligations": "tsx src/scripts/check-obligations.ts",
    "check-health": "node scripts/check-project-health.js",
    "fix:html": "npx eslint --fix \"**/*.{ts,tsx}\"",
    "fix:css": "node scripts/fix-css-warnings.js",
    "fix:infinite-loops": "node scripts/fix-infinite-loops.js",
    "fix:all": "npm run fix:html",
    "postinstall": "patch-package",
    "fix:radix": "node scripts/fix-all-radix-components.js",
    "type-check": "tsc --noEmit"
  },
